{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx prisma generate",
  "framework": "nextjs",
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
